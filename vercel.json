{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "WHALE_THRESHOLD": "100",
    "SUMMARY_INTERVAL": "60"
  },
  "functions": {
    "app.py": {
      "maxDuration": 30
    }
  }
}
